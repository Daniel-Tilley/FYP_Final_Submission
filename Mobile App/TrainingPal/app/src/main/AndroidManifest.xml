<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mobile_app.trainingpal.me">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/favicon_square"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/favicon_rounded"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ContainerActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".LoginActivity"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SignUpActivity"
            android:noHistory="true" />
        <activity android:name=".ViewTrainingLogActivity" />
        <activity android:name=".EditTrainingLogActivity" />
        <activity android:name=".EditUserActivity" />
        <activity android:name=".CreateTrainingLogActivity"></activity>
    </application>

</manifest>